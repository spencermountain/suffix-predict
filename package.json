{
  "name": "suffix-predict",
  "description": "learn transformations between two sets of words",
  "version": "0.0.1",
  "author": "Spencer Kelly <spencermountain@gmail.com> (http://spencermounta.in)",
  "main": "./src/index.js",
  "unpkg": "./builds/suffix-predict.js",
  "types": "types/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./builds/suffix-predict.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/spencermountain/suffix-predict.git"
  },
  "scripts": {
    "test": "tape \"./test/**/*.test.js\" | tap-dancer  --color always",
    "testb": "TESTENV=prod tape \"./test/**/*.test.js\" | tap-dancer  --color always",
    "watch": "amble ./scratch.js",
    "build": "rollup -c --silent"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  },
  "files": [
    "src/",
    "builds/"
  ],
  "eslintIgnore": [
    "builds/*.js"
  ],
  "dependencies": {
    "efrt": "2.5.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.1.2",
    "amble": "1.3.0",
    "rollup": "2.63.0",
    "rollup-plugin-terser": "7.0.2",
    "tap-dancer": "0.3.4",
    "tape": "5.4.0"
  },
  "license": "MIT"
}